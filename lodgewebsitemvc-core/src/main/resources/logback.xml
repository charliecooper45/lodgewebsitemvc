<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>
        %d{yyyy-MM-dd HH:mm:ss.SSS} %5p ${PID:- } --- [%15.15t{14}] %-40.40logger{39} : %m%n
      </pattern>
    </encoder>
  </appender>
  <logger name="uk.cooperca" level="TRACE"/>
  <root level="WARN">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>